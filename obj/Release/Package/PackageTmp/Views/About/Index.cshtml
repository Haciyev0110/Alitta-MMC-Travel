                                                <li><a href="@item.Social.Link2"><i class="fa fa-instagram"></i></a></li>
                                                <li><a href="@item.Social.Link3"><i class="fa fa-youtube"></i></a></li>
                                                <li><a href="@item.Social.Link4"><i class="fa fa-twitter"></i></a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="lower-content">
                                <h4><a>@item.Fullname</a></h4>
                                <div class="text">@item.Job</div>
                                <div class="mail"><i class="fa fa-envelope-o"><a>@item.Email</a></i></div>
                            </div>
                        </div>
                    </div>

                }
