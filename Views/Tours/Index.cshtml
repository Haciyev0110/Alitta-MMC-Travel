            </div>
        </div>
    </section><!-- blog breadcrumb version one end here -->
    <!-- popular destination strat -->
    <section class="blog-contents-version-one pt-100 pb-70">
        <div class="container">
            <div class="row">
                <!-- single hotels -->
                @foreach (var item in Model.tourPackets)
                {
                    <div class="col-md-4 col-sm-6 co-lxs-12 single-item">
                        <div class="single-hotels">
                            <div class="hotel-image">
                                <img src="~/Public/images/packages/@item.Image" alt="" class="border-raduis-3">
                            </div>
                            <div class="hotel-description">
                                <h4>@item.TourName</h4>
                                <p>Qiyməti <span>@item.Price/</span></p>
                                <div class="hotel-rating">
                                    @for (int i = 0; i < item.HotelStar; i++)
                                    {
                                        <i class="fa fa-star"></i>
                                    }
                                </div>
                                <div class="hotet-book-btn">
                                    <a href="~/Tours/Single/@item.ID" class="travel-primary-btn hvr-shutter-out-horizontal">show details</a>
                                </div>
                            </div>
                        </div>
                    </div><!--End single hotels -->

                }
            </div>
        </div>
    </section><!-- single popular destination  end-->
